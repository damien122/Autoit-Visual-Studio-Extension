{
	"name": "autoit",
	"displayName": "AutoIt",
	"description": "AutoIt",
	"icon": "img/ai_icon.png",
	"version": "0.0.5",
	"publisher": "Damien",
	"galleryBanner": {
		"color": "#254768",
		"theme": "light"
	},
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Languages",
		"Snippets"
	],
	"devDependencies": {
		"vscode": "^0.11.x",
		"typescript": "^1.6.2"
	},
	"main": "./extension",
	"activationEvents": [
			"onCommand:extension.runScript",
			"onCommand:extension.launchHelp",
			"onCommand:extension.launchInfo",
			"onCommand:extension.debugMsgBox"
	],
	"contributes": {
		"languages": [{
			"id": "autoit",
			"aliases": ["AutoIt", "autoit"],
			"extensions": [".au3"],
			"configuration": "./autoit-language-configuration.json"
		}],
		"commands": [{ 
			"command": "extension.runScript",
			"title": "AutoIt - Run Script" 
		}, {
			"command": "extension.launchHelp",
			"title": "AutoIt - Help"
		}, {
			"command": "extension.launchInfo",
			"title": "AutoIt - Au3Info"
		}, {
			"command": "extension.debugMsgBox",
			"title": "AutoIt - Debug to MsgBox"
		}],
		"grammars": [{
			"language": "autoit",
			"scopeName": "source.autoit",
			"path": "./syntaxes/autoit.tmLanguage"
		}],
		"snippets": [
			{
				"language": "autoit",
				"path": "./syntaxes/snippets.json"
			}
		],
		"keybindings": [{
			"command": "extension.runScript",
			"key": "f5",
			"when": "editorTextFocus"
		}, {
			"command": "extension.launchHelp",
			"key": "ctrl+f1",
			"when": "editorTextFocus"
		}, {
			"command": "extension.launchInfo",
			"key": "ctrl+f6",
			"when": "editorTextFocus"
		}, {
			"command": "extension.debugMsgBox",
			"key": "ctrl+shift+d",
			"when": "editorTextFocus"
		}],
		"repository": {
			"type": "git",
			"url": "https://github.com/loganch/AutoIt-VSCode.git"
		},
		"homepage": "https://github.com/loganch/AutoIt-VSCode/blob/master/README.md"
	}
}